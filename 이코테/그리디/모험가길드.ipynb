    "# 15:01 ~ 15:11\n",
    "N = 5\n",
    "people = [2,3,1,2,2]\n",
    "\n",
    "people.sort()\n",
    "group_count = 0\n",
    "last_group_i = -1\n",
    "for i, person in enumerate(people):\n",
    "    if i - last_group_i >= person:\n",
    "        group_count += 1\n",
    "        last_group_i = i\n",
    "\n",
    "print(group_count)"
   ]
