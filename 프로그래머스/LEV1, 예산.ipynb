    "# 13:15 ~ 13:18\n",
    "def solution(d, budget):\n",
    "    d.sort()\n",
    "\n",
    "    answer = 0\n",
    "    for department in d:\n",
    "        if budget >= department:\n",
    "            budget -= department\n",
    "            answer += 1\n",
    "        else:\n",
    "            break\n",
    "\n",
    "    return answer\n",
    "\n",
    "solution([2,2,3,3],10)"
   ]
  }
