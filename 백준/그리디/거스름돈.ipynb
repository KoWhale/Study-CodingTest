   "source": [
    "coins = [500,100,50,10,5,1]\n",
    "\n",
    "cost = int(input())\n",
    "remain = 1000 - cost\n",
    "\n",
    "count = 0\n",
    "for coin in coins:\n",
    "    count += remain // coin\n",
    "    remain = remain % coin\n",
    "\n",
    "print(count)"
   ]
  }
